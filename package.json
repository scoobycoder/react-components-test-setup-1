{
  "name": "minimal-react-webpack-babel-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "test:unit": "mocha --require babel-core/register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles 'src/**/*.spec.js'",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:snapshot": "jest --config ./test/jest.config.json",
    "test:snapshot:watch": "npm run test:snapshot -- --watch",
    "test:cypress": "start-server-and-test start http://localhost:8080 cypress",
    "cypress": "cypress run",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cypress": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "jest": "^22.4.2",
    "jsdom": "^11.11.0",
    "mocha": "^5.0.4",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "sinon": "^4.4.6",
    "start-server-and-test": "^1.1.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-env": "^1.6.1",
    "caniuse-lite": "^1.0.30000865",
    "domhandler": "^2.4.2",
    "electron-to-chromium": "^1.3.52",
    "loglevelnext": "^1.0.5",
    "lolex": "^2.7.1",
    "nearley": "^2.15.0",
    "nise": "^1.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
